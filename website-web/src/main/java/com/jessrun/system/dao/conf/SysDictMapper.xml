<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jessrun.system.dao.SysDictMapper" >
  
  <!-- 实体类映射mapper -->
  <resultMap id="SysDict" type="com.jessrun.system.domain.SysDict" >
  	     <result column="DICT_ID" property="dictId" jdbcType="VARCHAR" />
  	     <result column="DICT_NAME" property="dictName" jdbcType="VARCHAR" />
  	     <result column="DICT_CODE" property="dictCode" jdbcType="VARCHAR" />
  	     <result column="CREATOR_ID" property="creatorId" jdbcType="VARCHAR" />
  	     <result column="CREATOR_TIME" property="creatorTime" jdbcType="DATE" />
  	     <result column="EDITOR_ID" property="editorId" jdbcType="VARCHAR" />
  	     <result column="EDITOR_TIME" property="editorTime" jdbcType="DATE" />
  	     <result column="IS_DELETED" property="isDeleted" jdbcType="VARCHAR" />
  	     <result column="REMARK" property="remark" jdbcType="VARCHAR" />
  </resultMap>
  
  <!--查询条件-->
  <sql id="where_condition">
	  	     <if test="dictId != null">
	  			and DICT_ID = ${dictId}
	  		</if>
	  	     <if test="dictName != null">
	  			and DICT_NAME = ${dictName}
	  		</if>
	  	     <if test="dictCode != null">
	  			and DICT_CODE = ${dictCode}
	  		</if>
	  	     <if test="creatorId != null">
	  			and CREATOR_ID = ${creatorId}
	  		</if>
	  	     <if test="creatorTime != null">
	  			and CREATOR_TIME = ${creatorTime}
	  		</if>
	  	     <if test="editorId != null">
	  			and EDITOR_ID = ${editorId}
	  		</if>
	  	     <if test="editorTime != null">
	  			and EDITOR_TIME = ${editorTime}
	  		</if>
	  	     <if test="isDeleted != null">
	  			and IS_DELETED = ${isDeleted}
	  		</if>
	  	     <if test="remark != null">
	  			and REMARK = ${remark}
	  		</if>
  </sql>
  
 <!--查询单个Object--> 
  <select id="selectById" resultMap="SysDict">
  	select 
	  	       dictId ,       
	  	       dictName ,       
	  	       dictCode ,       
	  	       creatorId ,       
	  	       creatorTime ,       
	  	       editorId ,       
	  	       editorTime ,       
	  	       isDeleted ,       
	  	       remark        
	   from t_sys_dict  where 1=1  <include refid="where_condition"/>
  </select>
  
   <!--保存单个Object--> 
  <insert id="saveObject" parameterType="com.jessrun.system.domain.SysDict">
		insert into TB_CERTIFY_ACCOUNT (
	  	       DICT_ID ,       
	  	       DICT_NAME ,       
	  	       DICT_CODE ,       
	  	       CREATOR_ID ,       
	  	       CREATOR_TIME ,       
	  	       EDITOR_ID ,       
	  	       EDITOR_TIME ,       
	  	       IS_DELETED ,       
	  	       REMARK        
		)
		values(
	  	       #{dictId,jdbType=VARCHAR },
	  	       #{dictName,jdbType=VARCHAR },
	  	       #{dictCode,jdbType=VARCHAR },
	  	       #{creatorId,jdbType=VARCHAR },
	  	       #{creatorTime,jdbType=VARCHAR },
	  	       #{editorId,jdbType=VARCHAR },
	  	       #{editorTime,jdbType=VARCHAR },
	  	       #{isDeleted,jdbType=VARCHAR },
	  	       #{remark,jdbType=VARCHAR }
        )
	</insert>
 
  <!--唯一性校验-->
  <select id="checkDateIsExits" parameterType="java.lang.String"  resultType="java.lang.Integer">
  		select 
		    count(1) result
		 from t_sys_dict
		  <where>
	    </where>
  </select>
  
  <!--更新单个对象-->
  <update id="updateObject" parameterType="com.jessrun.system.domain.SysDict">
		update t_sys_dict  set
	  	      DICT_ID=#{dictId,jdbcType=VARCHAR} ,       
	  	      DICT_NAME=#{dictName,jdbcType=VARCHAR} ,       
	  	      DICT_CODE=#{dictCode,jdbcType=VARCHAR} ,       
	  	      CREATOR_ID=#{creatorId,jdbcType=VARCHAR} ,       
	  	      CREATOR_TIME=#{creatorTime,jdbcType=DATE} ,       
	  	      EDITOR_ID=#{editorId,jdbcType=VARCHAR} ,       
	  	      EDITOR_TIME=#{editorTime,jdbcType=DATE} ,       
	  	      IS_DELETED=#{isDeleted,jdbcType=VARCHAR} ,       
	  	      REMARK=#{remark,jdbcType=VARCHAR}        
	  	where
	  	    DICT_ID=#{dictId,jdbcType=VARCHAR}
  </update>
  
  <!--分页查询-->
  <select id="selectListByPage" resultMap="SysDict" parameterType="java.util.Map" >
	select 
	  	       DICT_ID ,       
	  	       DICT_NAME ,       
	  	       DICT_CODE ,       
	  	       CREATOR_ID ,       
	  	       CREATOR_TIME ,       
	  	       EDITOR_ID ,       
	  	       EDITOR_TIME ,       
	  	       IS_DELETED ,       
	  	       REMARK          
	   from t_sys_dict  where 1=1  <include refid="where_condition"/>
  </select>
</mapper>